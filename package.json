  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "global",
    "lib",
    "!.*"
  ],
  "main": "lib/polytype.cjs",
  "module": "lib/polytype.mjs",
  "types": "lib/polytype-module.d.ts",
  "directories": {
    "lib": "lib",
    "example": "example",
    "test": "test"
