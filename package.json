    "gulp-rename": "~1.4.0",
    "gulp-terser": "~1.2.0",
    "handlebars": "~4.1.2",
    "markdown-toc": "~1.2.0",
    "mocha": "~6.1.4",
    "mocha-bar": "~0.0.3",
    "nyc": "~14.1.1",
    "rollup": "~1.15.1",
    "subrequire": "~0.0.0",
    "typescript": "~3.5.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
